{
  "$schema": "https://tauri.app/schemas/2.0.0/capability.json",
  "identifier": "default",
  "description": "Default capabilities for PaperFuse Desktop",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "dialog:default",
    "dialog:allow-open",
    "shell:default",
    "shell:allow-open",
    "shell:allow-execute",
    "opener:default",
    "opener:allow-open-url"
  ]
}
